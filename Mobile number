#include <stdio.h>
#include <string.h>

int main() {
    char m[15];
    int i, valid = 1;

    scanf("%s", m);
    if (strlen(m) != 10) valid = 0;

    for (i = 0; m[i] && valid; i++)
        if (m[i] < '0' || m[i] > '9')
            valid = 0;

    printf(valid ? "Valid Mobile Number" : "Invalid Mobile Number");
    return 0;
}
