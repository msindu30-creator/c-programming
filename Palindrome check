#include <stdio.h>
#include <string.h>

int main() {
    char s[50];
    int i, flag = 1;

    scanf("%s", s);
    for (i = 0; i < strlen(s) / 2; i++)
        if (s[i] != s[strlen(s) - i - 1])
            flag = 0;

    if (flag)
        printf("Palindrome");
    else
        printf("Not Palindrome");

    return 0;
}
