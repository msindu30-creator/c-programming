#include <stdio.h>

int main() {
    FILE *f1, *f2;
    int id, x;

    f1 = fopen("data.txt", "r");
    f2 = fopen("temp.txt", "w");

    scanf("%d", &id);          // record id to delete
    while (fscanf(f1, "%d", &x) != EOF)
        if (x != id)
            fprintf(f2, "%d\n", x);

    fclose(f1);
    fclose(f2);

    remove("data.txt");
    rename("temp.txt", "data.txt");

    return 0;
}
